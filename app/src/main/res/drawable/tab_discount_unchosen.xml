<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/white_press" />
            <corners android:bottomLeftRadius="@dimen/dm_5dp" android:topLeftRadius="@dimen/dm_5dp" />
            <stroke android:width="@dimen/dm_1dp" android:color="@color/main_color" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/white" />
            <corners android:bottomLeftRadius="@dimen/dm_5dp" android:topLeftRadius="@dimen/dm_5dp" />
            <stroke android:width="@dimen/dm_1dp" android:color="@color/main_color" />
        </shape>
    </item>
</selector>